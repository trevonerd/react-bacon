{
  "name": "@3vo/my-npm-package",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node esbuild.config.js",
    "clean": "rm -rf ./dist",
    "dev": "nodemon",
    "lint": "eslint src/**/*.ts",
    "prettify": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ts": "tsc --noEmit",
    "test:ui": "vitest --ui",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@3vo/config-hub": "workspace:*",
    "@eslint/js": "^9.8.0",
    "@types/node": "22.0.0",
    "@vitest/coverage-v8": "^2.0.4",
    "@vitest/ui": "^2.0.4",
    "esbuild": "^0.23.0",
    "eslint": "^9.8.0",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.8.0",
    "nodemon": "^3.1.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0",
    "vitest": "^2.0.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
